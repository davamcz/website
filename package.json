  "scripts": {
    "dev": "dotenv -- next",
    "build": "dotenv -- next build",
    "start": "next start",
    "generate": "graphql-codegen --config codegen.yml",
    "nexus": "nexus-prisma-generate --project backend/prisma/prisma.yml --client backend/generated/prisma-client --output backend/generated/nexus-prisma",
    "prisma": "dotenv -- prisma deploy -p backend/prisma/prisma.yml",
    "prettier-check": "prettier --check \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\"",
    "prettier-fix": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\"",
    "lint-staged": "lint-staged",
    "generate:prisma": "prisma2 generate",
    "generate:nexus": "dotenv -- ts-node --skip-project --transpile-only nexus/schema",
    "generate:backend": "yarn run generate:prisma && yarn run generate:nexus",
    "postinstall": "yarn run generate:backend"
  },
  "pre-commit": "lint-staged",
  "resolutions": {
    "graphql": "14.6.0"
  },
